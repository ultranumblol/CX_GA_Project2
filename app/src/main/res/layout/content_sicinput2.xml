<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_sicinput2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="4dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="wgz.com.cx_ga_project.activity.SICInputActivity"
    tools:showIn="@layout/activity_sicinput2">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"

        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:id="@+id/tesllv"
            android:paddingBottom="40dp"
            ></LinearLayout>


    </ScrollView>
<ListView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="beforeDescendants"
    android:id="@+id/testlv"
    android:visibility="gone"
    ></ListView>
    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/testrv"
        android:visibility="gone"
        ></android.support.v7.widget.RecyclerView>
    <com.jude.easyrecyclerview.EasyRecyclerView
        android:id="@+id/sic_input_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:scrollbarStyle="insideOverlay"></com.jude.easyrecyclerview.EasyRecyclerView>

</RelativeLayout>
